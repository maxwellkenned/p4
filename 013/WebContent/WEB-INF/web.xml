<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns="http://xmlns.jcp.org/xml/ns/javaee"
	xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd"
	id="WebApp_ID" version="3.1">
	<display-name>013</display-name>

	<servlet>
		<servlet-name>semestres</servlet-name>
		<servlet-class>nassau.Semestre</servlet-class>
	</servlet>

	<servlet>
		<servlet-name>disciplinas</servlet-name>
		<servlet-class>nassau.Disciplina</servlet-class>
	</servlet>

	<servlet>
		<servlet-name>matricula</servlet-name>
		<servlet-class>nassau.Matricula</servlet-class>
	</servlet>

	<servlet-mapping>
		<servlet-name>semestres</servlet-name>
		<url-pattern>/Semestre</url-pattern>
	</servlet-mapping>

	<servlet-mapping>
		<servlet-name>disciplinas</servlet-name>
		<url-pattern>/Disciplinas</url-pattern>
	</servlet-mapping>

	<servlet-mapping>
		<servlet-name>matricula</servlet-name>
		<url-pattern>/Matricula</url-pattern>
	</servlet-mapping>

</web-app>